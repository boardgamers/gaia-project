    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "mocha": "^5.0.4",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "ts-node": "^5.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.7.2"
  }
