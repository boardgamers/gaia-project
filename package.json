{
  "name": "@gaia-project/engine",
  "version": "1.3.6",
  "description": "Javascript engine for project gaia",
  "main": "dist/index.js",
  "types": "index.ts",
  "source": "index.ts",
  "repository": "git@github.com:donkeytech/gaia-engine.git",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist",
    "index.d.ts",
    "index.ts",
    "server.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "nodemon": "nodemon -e ts --exec npm start",
    "prepublishOnly": "npm run clean && tsc",
    "postpublish": "npm run clean",
    "start": "ts-node server.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -Rf dist",
    "lint": "tslint -c tslint.json index.ts src/**/*.ts",
    "test": "npm run lint && npm run just-test",
    "just-test": "mocha -r ts-node/register src/**/*.spec.ts src/*.spec.ts"
  },
