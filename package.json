{
  "name": "@gaia-project/engine",
  "version": "4.4.3",
  "description": "Javascript engine for project gaia",
  "main": "dist/index.js",
  "types": "index.ts",
  "source": "index.ts",
  "repository": "git@github.com:donkeytech/gaia-engine.git",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist",
    "index.d.ts",
    "index.ts",
    "tsconfig.json",
    "wrapper.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run just-test && npm run clean && tsc",
    "build": "npm run clean && tsc",
    "clean": "rm -Rf dist",
    "lint": "eslint . --ext .ts",
    "fix-lint": "eslint . --ext .ts --fix",
    "prettier": "npx --yes -- prettier --write .",
    "prettier-check": "npx --yes -- prettier --check .",
    "test": "npm run lint && npm run prettier-check && npm run just-test",
    "just-test": "mocha -r ts-node/register src/**/*.spec.ts src/*.spec.ts"
  },
  "contributors": [
    "coyotte508 <coyotte508@gmail.com>",
